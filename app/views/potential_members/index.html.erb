<h1>Listing potential_members</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Zip code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @potential_members.each do |potential_member| %>
      <tr>
        <td><%= potential_member.first_name %></td>
        <td><%= potential_member.last_name %></td>
        <td><%= potential_member.email %></td>
        <td><%= potential_member.zip_code %></td>
        <td><%= link_to 'Show', potential_member %></td>
        <td><%= link_to 'Edit', edit_potential_member_path(potential_member) %></td>
        <td><%= link_to 'Destroy', potential_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Potential member', new_potential_member_path %>
